import pandas as pd
import matplotlib.pyplot as plt

# Step: Load the data
data = pd.read_csv('data.csv')

# Step: Inspecting the first few rows to understand the data structure
print(data.head())

# Use the correct column name representing flower types
flower_counts = data['variety'].value_counts()

# Step: Plot the data
plt.figure(figsize=(10, 6))
flower_counts.plot(kind='bar', color='skyblue')
plt.title('Flower Type Distribution')
plt.xlabel('Flower Type')
plt.ylabel('Count')
plt.xticks(rotation=45)
plt.tight_layout()

# Save the plot
plt.savefig('output.png')
plt.show()

print("Graph has been generated and saved as output.png")
